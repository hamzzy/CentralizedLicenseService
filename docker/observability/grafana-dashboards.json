{
  "id": null,
  "uid": "license-service-observability",
  "title": "License Service - Complete Observability",
  "tags": [
    "license-service",
    "django",
    "observability"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 0,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate by Endpoint",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m])) by (method, endpoint)",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "id": 2,
      "title": "Request Duration (p50, p95, p99)",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p50 - {{endpoint}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p95 - {{endpoint}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint))",
          "legendFormat": "p99 - {{endpoint}}",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 3,
      "title": "HTTP Status Codes",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"2..\"}[5m])) by (status_code)",
          "legendFormat": "{{status_code}}",
          "refId": "A"
        },
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"4..\"}[5m])) by (status_code)",
          "legendFormat": "{{status_code}}",
          "refId": "B"
        },
        {
          "expr": "sum(rate(http_requests_total{status_code=~\"5..\"}[5m])) by (status_code)",
          "legendFormat": "{{status_code}}",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
